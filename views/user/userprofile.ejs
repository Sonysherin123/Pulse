    <script>
        function getElementValue(id) {
            const element = document.getElementById(id);
            return element.value;
        }
    
        function validateInput(inputId, pattern, errorId, errorMessage) {
            const inputValue = getElementValue(inputId);
            const errorElement = document.getElementById(errorId);
    
            if (inputValue.trim() === '') {
                errorElement.innerText = errorMessage + ' is required.';
                return false;
            } else if (pattern && !pattern.test(inputValue)) {
                errorElement.innerText = 'Please enter a valid ' + errorMessage.toLowerCase() + '.';
                return false;
            } else {
                errorElement.innerText = '';
                return true;
            }
        }
    
        document.addEventListener('DOMContentLoaded', function () {
            const nameInput = document.getElementById('name');
            const phoneInput = document.getElementById('phone');
            const altPhoneInput = document.getElementById('altPhone');
            const pincodeInput = document.getElementById('pincode');
            const landMarkInput = document.getElementById('landMark');
            const cityInput = document.getElementById('city');
            const stateInput = document.getElementById('state');
            const addressTypeInput = document.getElementById('addressType');
            const homeAddressInput = document.getElementById('homeAddress');
            const submitButton = document.getElementById('submitButton');
    
            // Use input event for real-time validation
            nameInput.addEventListener('input', () => validateInput('name', /^[A-Z][a-zA-Z]+\s[A-Z][a-zA-Z]+$/, 'nameError', 'Full Name'));
            phoneInput.addEventListener('input', () => validateInput('phone', /^(91)?0?[6-9]\d{9}$/, 'phoneError', 'Phone'));
            altPhoneInput.addEventListener('input', () => validateInput('altPhone', /^(91)?0?[6-9]\d{9}$/, 'altPhoneError', 'Alternate Phone'));
            pincodeInput.addEventListener('input', () => validateInput('pincode', /^[1-9][0-9]{5}(?:\s[0-9]{3})?$/, 'pincodeError', 'Pincode (6 digits)'));
            landMarkInput.addEventListener('input', () => validateInput('landMark', null, 'landMarkError', 'Landmark'));
            cityInput.addEventListener('input', () => validateInput('city', /^[a-zA-Z\s]+$/, 'cityError', 'City'));
            stateInput.addEventListener('input', () => validateInput('state', /^[a-zA-Z\s]+$/, 'stateError', 'State'));
            addressTypeInput.addEventListener('change', () => validateInput('addressType', null, 'addressTypeError', 'Address Type'));
            homeAddressInput.addEventListener('input', () => validateInput('homeAddress', null, 'homeAddressError', 'Address'));
    
            // Submit form on button click
            submitButton.addEventListener('click', validateForm);
        });
    
        function validateForm(e) {
            const isValidName = validateInput('name', /^[A-Z][a-zA-Z]+\s[A-Z][a-zA-Z]+$/, 'nameError', 'Full Name');
            const isValidPhone = validateInput('phone', /^(91)?0?[6-9]\d{9}$/, 'phoneError', 'Phone');
            const isValidAltPhone = validateInput('altPhone', /^(91)?0?[6-9]\d{9}$/, 'altPhoneError', 'Alternate Phone');
            const isValidPincode = validateInput('pincode', /^[1-9][0-9]{5}(?:\s[0-9]{3})?$/, 'pincodeError', 'Pincode (6 digits)');
            const isValidLandMark = validateInput('landMark', null, 'landMarkError', 'Landmark');
            const isValidCity = validateInput('city', /^[a-zA-Z\s]+$/, 'cityError', 'City');
            const isValidState = validateInput('state', /^[a-zA-Z\s]+$/, 'stateError', 'State');
            const isValidAddressType = validateInput('addressType', null, 'addressTypeError', 'Address Type');
            const isValidHomeAddress = validateInput('homeAddress', null, 'homeAddressError', 'Address');
    
            if (!isValidName || !isValidPhone || !isValidAltPhone || !isValidPincode || !isValidLandMark || !isValidCity || !isValidState || !isValidAddressType || !isValidHomeAddress) {
                e.preventDefault();
                document.getElementById('mainError').innerText = 'Cannot submit form. Please fix errors.';
                return false;
            }
            return true;
        }
    </script>
    

    

