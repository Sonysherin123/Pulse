<script>
    $(document).ready(function() {
        $('.datepicker').datepicker({
            format: 'yyyy-mm-dd',
            autoclose: true,
            todayHighlight: true
        });

        // Function to send updated dates to the server
        function updateDates() {
            var startDate = $("#Startdate").val();
            var endDate = $("#Enddate").val();

            console.log("Start Date:", startDate);
            console.log("End Date:", endDate);

            // Make an AJAX request to send the updated dates to the server
            $.ajax({
                url: "/update-dates", // Replace with your server endpoint
                method: "POST",
                data: {
                    startDate: startDate,
                    endDate: endDate
                },
                success: function(response) {
                    // Handle success
                    console.log("Dates updated successfully");
                },
                error: function(xhr, status, error) {
                    // Handle error
                    console.error("Error updating dates:", error);
                }
            });
        }

        // Event listener for the change event of the start date input
        $("#Startdate").on("change", function() {
            updateDates();
        });

        // Event listener for the change event of the end date input
        $("#Enddate").on("change", function() {
            updateDates();
        });
    });
</script>

    
